{
  "name": "AutoCal",
  "version": "1.0.0",
  "description": "Reads emails and creates Google Calendar events using AI.",
  "permissions": ["identity", "storage", "activeTab"],
  "host_permissions": ["https://mail.google.com/*", "https://www.googleapis.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/logo16.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  },
  "oauth2": {
    "client_id": "242469590577-9hp4id5jkb8sj343dlh8dc4ub049v4th.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "manifest_version": 3
}